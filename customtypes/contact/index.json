{
  "id": "contact",
  "label": "Contact",
  "repeatable": false,
  "status": true,
  "json": {
    "Main": {
      "page_title": {
        "type": "StructuredText",
        "config": {
          "label": "Page Title",
          "placeholder": "Title of the site header",
          "allowTargetBlank": false,
          "single": "heading1"
        }
      },
      "logo": {
        "type": "Image",
        "config": {
          "label": "Logo",
          "constraint": {
            "width": 500,
            "height": 500
          },
          "thumbnails": []
        }
      },
      "heading_title": {
        "type": "StructuredText",
        "config": {
          "label": "Heading Title",
          "placeholder": "Content heading title",
          "allowTargetBlank": false,
          "single": "heading2"
        }
      },
      "meta_title": {
        "type": "Text",
        "config": {
          "label": "Meta Title",
          "placeholder": "The title that appears in Google"
        }
      },
      "meta_description": {
        "type": "Text",
        "config": {
          "label": "Meta Description",
          "placeholder": "Site description in Google"
        }
      },
      "content": {
        "type": "StructuredText",
        "config": {
          "label": "Content",
          "placeholder": "Main content of page",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,list-item,o-list-item"
        }
      },
      "contact_details": {
        "type": "StructuredText",
        "config": {
          "label": "Contact Details",
          "placeholder": "Phone number, email address",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading4,heading5,heading6,strong,em,hyperlink,list-item,o-list-item,heading3,image"
        }
      },
      "contact_success_msg": {
        "type": "StructuredText",
        "config": {
          "label": "Contact Success Message",
          "placeholder": "Message when customer submits form",
          "allowTargetBlank": true,
          "multi": "paragraph,heading5,heading6,strong,em,hyperlink,heading4"
        }
      },
      "address": {
        "type": "StructuredText",
        "config": {
          "label": "Contact Address",
          "placeholder": "The address of the salon.",
          "allowTargetBlank": true,
          "multi": "heading4,heading5,heading6,paragraph,strong,hyperlink,image,list-item"
        }
      },
      "map_location": {
        "type": "GeoPoint",
        "config": {
          "label": "Google Maps Embed URI"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "footer": {
              "type": "SharedSlice"
            }
          }
        }
      }
    }
  }
}